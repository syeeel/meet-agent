{
  "name": "meet-agent",
  "version": "2.0.0",
  "description": "AI meeting agent using Recall.ai, Hedra Avatar, and Google Gemini Live API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js start",
    "dev": "tsc && node dist/index.js dev",
    "dev:watch": "tsc --watch & node dist/index.js dev",
    "download-files": "node -e \"require('@livekit/agents-plugin-silero').VAD.load()\""
  },
  "keywords": [
    "meeting",
    "ai",
    "recall",
    "gemini",
    "hedra",
    "livekit",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@livekit/agents": "1.0.42",
    "@livekit/agents-plugin-google": "1.0.42",
    "@livekit/agents-plugin-hedra": "1.0.42",
    "@livekit/agents-plugin-silero": "1.0.42",
    "@livekit/rtc-node": "^0.13.24",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "livekit-server-sdk": "^2.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
